<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:onClick="onsiteclick"
    tools:context="com.example.robuntu.listoftasks.MainScreen">

    <Button
        android:id="@+id/button"
        android:layout_width="292dp"
        android:layout_height="46dp"
        android:layout_above="@+id/button2"
        android:layout_alignLeft="@+id/button2"
        android:layout_alignStart="@+id/button2"
        android:layout_marginBottom="15dp"
        android:onClick="onmonday"
        android:text="Monday"
        tools:layout_editor_absoluteX="46dp"
        tools:layout_editor_absoluteY="200dp" />

    <Button
        android:id="@+id/button2"
        android:layout_width="292dp"
        android:layout_height="46dp"
        android:layout_above="@+id/button3"
        android:layout_alignLeft="@+id/button3"
        android:layout_alignStart="@+id/button3"
        android:layout_marginBottom="15dp"
        android:onClick="ontuesday"
        android:text="Tuesday"
        tools:layout_editor_absoluteX="46dp"
        tools:layout_editor_absoluteY="259dp" />

    <Button
        android:id="@+id/button3"
        android:layout_width="292dp"
        android:layout_height="46dp"
        android:layout_above="@+id/button4"
        android:layout_alignLeft="@+id/button4"
        android:layout_alignStart="@+id/button4"
        android:layout_marginBottom="12dp"
        android:onClick="onwednesday"
        android:text="Wednesday"
        tools:layout_editor_absoluteX="46dp"
        tools:layout_editor_absoluteY="319dp" />

    <Button
        android:id="@+id/button4"
        android:layout_width="292dp"
        android:layout_height="46dp"
        android:layout_above="@+id/button5"
        android:layout_alignLeft="@+id/button5"
        android:layout_alignStart="@+id/button5"
        android:layout_marginBottom="16dp"
        android:onClick="onthursday"
        android:text="Thursday"
        tools:layout_editor_absoluteX="46dp"
        tools:layout_editor_absoluteY="382dp" />

    <Button
        android:id="@+id/button5"
        android:layout_width="292dp"
        android:layout_height="46dp"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="49dp"
        android:onClick="onfriday"
        android:text="Friday"
        tools:layout_editor_absoluteX="46dp"
        tools:layout_editor_absoluteY="439dp" />

    <Button
        android:id="@+id/button6"
        android:layout_width="110dp"
        android:layout_height="60dp"
        android:layout_below="@+id/textView16"
        android:layout_toLeftOf="@+id/textView16"
        android:layout_toStartOf="@+id/textView16"
        android:onClick="onschoolsite"
        android:text="School Site"
        tools:layout_editor_absoluteX="32dp"
        tools:layout_editor_absoluteY="16dp" />

    <Button
        android:id="@+id/button7"
        android:layout_width="110dp"
        android:layout_height="60dp"
        android:layout_alignTop="@+id/button6"
        android:layout_centerHorizontal="true"
        android:onClick="onmap"
        android:text="School Map"
        tools:layout_editor_absoluteX="147dp"
        tools:layout_editor_absoluteY="16dp" />

    <Button
        android:id="@+id/button8"
        android:layout_width="110dp"
        android:layout_height="60dp"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_alignTop="@+id/button7"
        android:layout_marginEnd="13dp"
        android:layout_marginRight="13dp"
        android:onClick="ongoogleclassroom"
        android:text="Google Classroom"
        tools:layout_editor_absoluteX="278dp"
        tools:layout_editor_absoluteY="16dp" />

    <Button
        android:id="@+id/button9"
        android:layout_width="110dp"
        android:layout_height="60dp"
        android:layout_alignLeft="@+id/button7"
        android:layout_alignStart="@+id/button7"
        android:layout_below="@+id/button7"
        android:onClick="onmeme"
        android:text="Meme of the Day" />

    <Button
        android:id="@+id/button10"
        android:layout_width="110dp"
        android:layout_height="60dp"
        android:layout_alignBaseline="@+id/button9"
        android:layout_alignBottom="@+id/button9"
        android:layout_alignLeft="@+id/button8"
        android:layout_alignStart="@+id/button8"
        android:onClick="onspiritvideo"
        android:text="St Rob's Spirit" />

    <Button
        android:id="@+id/buttonLogOut"
        android:layout_width="110dp"
        android:layout_height="60dp"
        android:layout_alignLeft="@+id/button6"
        android:layout_alignStart="@+id/button6"
        android:layout_alignTop="@+id/button10"
        android:onClick="onLogOut"
        android:text="Log-Off" />

    <TextView
        android:id="@+id/textView16"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="13dp"
        android:text="Useful Buttons"
        android:textAppearance="@android:style/TextAppearance.Large" />

    <TextView
        android:id="@+id/textView17"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/button10"
        android:layout_centerHorizontal="true"
        android:text="Days Of The Week"
        android:textAppearance="@android:style/TextAppearance.Large" />


</RelativeLayout>
