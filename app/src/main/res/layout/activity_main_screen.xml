<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:onClick="onsiteclick"
    tools:context="com.example.robuntu.listoftasks.MainScreen">

    <Button
        android:id="@+id/button"
        android:layout_width="292dp"
        android:layout_height="46dp"
        android:layout_above="@+id/button2"
        android:layout_alignLeft="@+id/button2"
        android:layout_alignStart="@+id/button2"
        android:layout_marginBottom="26dp"
        android:onClick="onmonday"
        android:text="Monday"
        tools:layout_editor_absoluteX="46dp"
        tools:layout_editor_absoluteY="200dp" />

    <Button
        android:id="@+id/button2"
        android:layout_width="292dp"
        android:layout_height="46dp"
        android:layout_above="@+id/button3"
        android:layout_alignLeft="@+id/button3"
        android:layout_alignStart="@+id/button3"
        android:layout_marginBottom="15dp"
        android:onClick="ontuesday"
        android:text="Tuesday"
        tools:layout_editor_absoluteX="46dp"
        tools:layout_editor_absoluteY="259dp" />

    <Button
        android:id="@+id/button3"
        android:layout_width="292dp"
        android:layout_height="46dp"
        android:layout_above="@+id/button4"
        android:layout_alignLeft="@+id/button4"
        android:layout_alignStart="@+id/button4"
        android:layout_marginBottom="12dp"
        android:onClick="onwednesday"
        android:text="Wednesday"
        tools:layout_editor_absoluteX="46dp"
        tools:layout_editor_absoluteY="319dp" />

    <Button
        android:id="@+id/button4"
        android:layout_width="292dp"
        android:layout_height="46dp"
        android:layout_above="@+id/button5"
        android:layout_alignLeft="@+id/button5"
        android:layout_alignStart="@+id/button5"
        android:layout_marginBottom="16dp"
        android:onClick="onthursday"
        android:text="Thursday"
        tools:layout_editor_absoluteX="46dp"
        tools:layout_editor_absoluteY="382dp" />

    <Button
        android:id="@+id/button5"
        android:layout_width="292dp"
        android:layout_height="46dp"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="35dp"
        android:onClick="onfriday"
        android:text="Friday"
        tools:layout_editor_absoluteX="46dp"
        tools:layout_editor_absoluteY="439dp" />

    <Button
        android:id="@+id/button6"
        android:layout_width="90dp"
        android:layout_height="108dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginLeft="12dp"
        android:layout_marginStart="12dp"
        android:layout_marginTop="31dp"
        android:onClick="onschoolsite"
        android:text="School Site"
        tools:layout_editor_absoluteX="32dp"
        tools:layout_editor_absoluteY="16dp" />

    <Button
        android:id="@+id/button7"
        android:layout_width="90dp"
        android:layout_height="108dp"
        android:layout_alignTop="@+id/button6"
        android:layout_centerHorizontal="true"
        android:text="Map"
        tools:layout_editor_absoluteX="147dp"
        tools:layout_editor_absoluteY="16dp" />

    <Button
        android:id="@+id/button8"
        android:layout_width="90dp"
        android:layout_height="108dp"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_alignTop="@+id/button7"
        android:layout_marginEnd="21dp"
        android:layout_marginRight="21dp"
        android:onClick="ongoogleclassroom"
        android:text="Google Classroom"
        tools:layout_editor_absoluteX="278dp"
        tools:layout_editor_absoluteY="16dp" />

</RelativeLayout>
